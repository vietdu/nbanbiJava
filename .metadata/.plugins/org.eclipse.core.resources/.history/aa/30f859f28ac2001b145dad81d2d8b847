var images = ["anh/kiquan1.jpg", "anh/kiquan2.jpg", "anh/kiquan3.jpg", "anh/kiquan5.jpg",];
  $(function () {
      var i = 0;
      $("#dvImage").css("background-image", "url(" + images[i] + ")");
      setInterval(function () {
          i++;
          if (i == images.length) {
              i = 0;
          }
          $("#dvImage").fadeOut("slow", function () {
              $(this).css("background-image", "url(" + images[i] + ")");
              $(this).fadeIn("slow");
          });
      }, 20000);
  });
           
